  <step>
    <name>Get table mirror update  treatment</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>etl_db</connection>
    <sql>SELECT
    CASE
        WHEN NOT mirror_tables THEN ${QF_MIRROR_ALG_DO_NOT_MIRROR}
        WHEN can_insert_rows AND NOT can_update_rows AND NOT can_delete_rows AND NOT use_custom_mirror_algorithm THEN ${QF_MIRROR_ALG_INSERT_ONLY_GENERIC}
        WHEN can_insert_rows AND NOT can_update_rows AND NOT can_delete_rows AND     use_custom_mirror_algorithm THEN ${QF_MIRROR_ALG_INSERT_ONLY_CUSTOM}
        WHEN can_insert_rows AND     can_update_rows AND NOT can_delete_rows AND     use_custom_mirror_algorithm THEN ${QF_MIRROR_ALG_INSERT_OR_UPDATE_CUSTOM}
    ELSE -1
    END AS "mirror_treatment"
FROM
    etl.table_meta
WHERE
    source_db_id = ${PARAM_SOURCE_DB_ID} AND
    source_schema_name = '${PARAM_SOURCE_SCHEMA}' AND
    source_table_name ='${PARAM_SOURCE_TABLE}'

-- The rest of this query is present only to treat the case where there is no row present for the specified database,
-- schema and table. In this case, we still want to return a value for "mirror_treatment", in this case the value:
--
--     ${QF_MIRROR_ALG_DO_NOT_MIRROR}:

UNION ALL
SELECT
    ${QF_MIRROR_ALG_DO_NOT_MIRROR} AS "mirror_treatment"
WHERE
    NOT EXISTS (
        SELECT
            1
        FROM
            etl.table_meta
        WHERE
            source_db_id = ${PARAM_SOURCE_DB_ID} AND
            source_schema_name = '${PARAM_SOURCE_SCHEMA}' AND
            source_table_name ='${PARAM_SOURCE_TABLE}'
     )</sql>
