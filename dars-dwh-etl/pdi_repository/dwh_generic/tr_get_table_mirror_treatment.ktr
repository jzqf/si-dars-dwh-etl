  <step>
    <name>Get table mirror update  treatment</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>etl_db</connection>
    <sql>SELECT&#xa;    CASE&#xa;        WHEN NOT mirror THEN &#x24;&#x7b;QF_MIRROR_ALG_DO_NOT_MIRROR&#x7d;&#xa;        WHEN can_insert_rows AND NOT can_update_rows AND NOT can_delete_rows AND NOT use_custom_mirror_algorithm THEN &#x24;&#x7b;QF_MIRROR_ALG_INSERT_ONLY_GENERIC&#x7d;&#xa;        WHEN can_insert_rows AND NOT can_update_rows AND NOT can_delete_rows AND     use_custom_mirror_algorithm THEN &#x24;&#x7b;QF_MIRROR_ALG_INSERT_ONLY_CUSTOM&#x7d;&#xa;        WHEN can_insert_rows AND     can_update_rows AND NOT can_delete_rows AND     use_custom_mirror_algorithm THEN &#x24;&#x7b;QF_MIRROR_ALG_INSERT_OR_UPDATE_CUSTOM&#x7d;&#xa;    ELSE -1&#xa;    END AS &#x22;mirror_treatment&#x22;&#xa;FROM&#xa;    etl.table_config&#xa;WHERE&#xa;    source_db_id &#x3d; &#x24;&#x7b;PARAM_SOURCE_DB_ID&#x7d; AND&#xa;    schema_name &#x3d; &#x27;&#x24;&#x7b;PARAM_SCHEMA&#x7d;&#x27; AND&#xa;    table_name &#x3d;&#x27;&#x24;&#x7b;PARAM_TABLE&#x7d;&#x27;&#xa;&#xa;-- The rest of this query is present only to treat the case where there is no row present for the specified database,&#xa;-- schema and table. In this case, we still want to return a value for &#x22;mirror_treatment&#x22;, in this case the value&#x3a;&#xa;--&#xa;--     &#x24;&#x7b;QF_MIRROR_ALG_DO_NOT_MIRROR&#x7d;&#x3a;&#xa;&#xa;UNION ALL&#xa;SELECT&#xa;    &#x24;&#x7b;QF_MIRROR_ALG_DO_NOT_MIRROR&#x7d; AS &#x22;mirror_treatment&#x22;&#xa;WHERE&#xa;    NOT EXISTS &#x28;&#xa;        SELECT&#xa;            1&#xa;        FROM&#xa;            etl.table_config&#xa;        WHERE&#xa;            source_db_id &#x3d; &#x24;&#x7b;PARAM_SOURCE_DB_ID&#x7d; AND&#xa;            schema_name &#x3d; &#x27;&#x24;&#x7b;PARAM_SCHEMA&#x7d;&#x27; AND&#xa;            table_name &#x3d;&#x27;&#x24;&#x7b;PARAM_TABLE&#x7d;&#x27;&#xa;     &#x29;&#xa;&#x3b;</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
