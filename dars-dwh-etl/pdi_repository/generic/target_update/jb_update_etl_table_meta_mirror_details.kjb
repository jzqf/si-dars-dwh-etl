      <sql>INSERT INTO ${QF_ETL_DB_SCHEMA}.target_table_update (
    table_meta_id,
    created_on,
    algorithm_id,
    num_inserts,
    num_updates,
    num_rows_processed,
    elapsed_time_millis,
    num_rows_processed_per_sec,
    insert_id_colname, 
    insert_id_maxvalue,
    last_updated_on_colname,
    last_updated_on_maxvalue
)
VALUES (
    ${PARAM_TABLE_META_ID},
    now() at time zone 'utc',  -- LOCALTIMESTAMP
    ${PARAM_TABLE_MIRROR_ALGORITHM_ID},

    -- Values that are not set here should be SQL NULL, but it is possible that this is not so. If this occurs,
    -- I may have to check for a guard value and then use NULLIF(&lt;update value>, &lt;guard value>).
    ${PARAM_UPDATE_TABLE_NUM_INSERTS},
    ${PARAM_UPDATE_TABLE_NUM_UPDATES},
    ${PARAM_UPDATE_TABLE_NUM_ROWS},
    ${QF_UPDATE_TABLE_ELAPSED_MILLISECONDS},
    ${QF_UPDATE_TABLE_ROWS_PER_SEC},

    NULLIF('${PARAM_TARGET_INSERT_ID_COLNAME}', ''),
    NULLIF(${QF_TARGET_TABLE_MAX_INSERT_ID}, ${QF_POSITIVE_INTEGER_NOT_SET_VALUE}),
    NULLIF('${PARAM_TARGET_LAST_UPDATED_ON_COLNAME}', ''),
    NULLIF(TIMESTAMP '${QF_TARGET_TABLE_MAX_LAST_UPDATED_ON}', TIMESTAMP '${QF_TIMESTAMP_NOT_SET_VALUE}')
)</sql>
