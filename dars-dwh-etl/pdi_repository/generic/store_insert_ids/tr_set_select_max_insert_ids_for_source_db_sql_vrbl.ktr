  <step>
    <name>Generate SQL to fetch MAX(archive_ID) for each source table</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>etl_db</connection>
    <sql>WITH
    select_text AS (
        SELECT
            -- COALESCE used in case the table is empty.
            'SELECT ' || t.table_meta_id || ' AS "table_meta_id", ' ||
            'COALESCE(MAX(' || c.source_column_name || '), ${QF_ARCHIVE_ID_LOWER_BOUND}) AS "max_insert_id" ' ||
            'FROM ' || t.source_schema_name || '.' || t.source_table_name AS "select_sql_for_one_table"
        FROM 
            etl.table_meta t
        INNER JOIN
            etl.column_meta c ON c.table_meta_id = t.table_meta_id 
        WHERE
            t.source_db_id = ${PARAM_SOURCE_DB_ID} AND
            t.target_db_id = ${PARAM_TARGET_DB_ID} AND
            c.is_insert_id_column = true
    )
SELECT
--    'START TRANSACTION;' ||  chr(10) || 
--    'START TRANSACTION ISOLATION LEVEL REPEATABLE READ;' ||  chr(10) || 
--    'SET TRANSACTION ISOLATION LEVEL REPEATABLE READ;' ||  chr(10) || 
    array_to_string(array_agg(select_sql_for_one_table), chr(10) || ' UNION ALL ') || ';'  -- UNION ALL is faster than UNION
--    ||  chr(10) || 'COMMIT;' 
AS "select_sql_for_source_db"
FROM
    select_text</sql>
    <limit>0</limit>
    <lookup />
