      <description />
      <type>SQL</type>
      <sql>
-- TODO: All of the SQL in this step needs to be performed in a transaction.
--       To execute this SQL in a transaction, we need to execute it somehow in a transformation (which supports transactions), 
--       or use the Enterprise version of PDI, which supports transactions for jobs. I  have selected the job entry option:
--       "Send SQL as single statement?". It is not clear to me from the documentation, but this may also ensure that all of
--       the SQL here is committed in a single transaction. However, I am not sure of this behaviour. 

---- Update the "row_deleted_on" column of the "can delete row" table rows to contain the current UTC timestamp.
--UPDATE
--    ${PARAM_CAN_DELETE_ROW_SCHEMA_NAME}.${PARAM_CAN_DELETE_ROW_TABLE_NAME} cdr_tbl 
--SET
--    ${QF_CDR_DELETED_ON_COLNAME}=CURRENT_TIMESTAMP AT TIME ZONE 'UTC' 
--FROM
--    ${PARAM_SOURCE_SCHEMA}.${PARAM_SOURCE_TABLE} src_tbl
--WHERE
--    cdr_tbl.${QF_CDR_TABLE_NAME_COLNAME}='${PARAM_SOURCE_SCHEMA}.${PARAM_SOURCE_TABLE}' AND
--    cdr_tbl.${QF_CDR_INSERT_ID_COLNAME}=src_tbl.${PARAM_SOURCE_INSERT_ID_COLNAME};

---- Delete the ${PARAM_SOURCE_SCHEMA}.${PARAM_SOURCE_TABLE} rows:
--DELETE FROM
--    ${PARAM_SOURCE_SCHEMA}.${PARAM_SOURCE_TABLE} src_tbl
--USING    -- POSTGRESQL-SPECIFIC: USING clause? If you want to use standard SQL, use a sub-select here.
--         --                                    See the PostgreSQL DELETE FROM documentation.
--    ${PARAM_CAN_DELETE_ROW_SCHEMA_NAME}.${PARAM_CAN_DELETE_ROW_TABLE_NAME} cdr_tbl
--WHERE
--    cdr_tbl.${QF_CDR_TABLE_NAME_COLNAME}='${PARAM_SOURCE_SCHEMA}.${PARAM_SOURCE_TABLE}' AND
--    cdr_tbl.${QF_CDR_INSERT_ID_COLNAME}=src_tbl.${PARAM_SOURCE_INSERT_ID_COLNAME};


---- This is for testing only. It deletes any "can delete row" table rows that are linked to the source table being treated here.
--DELETE FROM
--    ${PARAM_CAN_DELETE_ROW_SCHEMA_NAME}.${PARAM_CAN_DELETE_ROW_TABLE_NAME} cdr_tbl 
--USING
--    ${PARAM_SOURCE_SCHEMA}.${PARAM_SOURCE_TABLE} src_tbl
--WHERE
--    cdr_tbl.${QF_CDR_TABLE_NAME_COLNAME}='${PARAM_SOURCE_SCHEMA}.${PARAM_SOURCE_TABLE}' AND
--    cdr_tbl.${QF_CDR_INSERT_ID_COLNAME}=src_tbl.${PARAM_SOURCE_INSERT_ID_COLNAME};
</sql>
