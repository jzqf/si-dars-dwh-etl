      <is_usevars>Y</is_usevars>
      <custom_sql>SELECT
    source_schema_name AS "source_schema",
    source_table_name  AS "source_table",
    source_schema_name || '.' || source_table_name AS "source_schema_dot_table",
    target_schema_name AS "target_schema",
    target_table_name  AS "target_table", 
    target_schema_name || '.' || target_table_name AS "target_schema_dot_table"
FROM
    etl.table_meta
WHERE
    source_db_id=${PARAM_SOURCE_DB_ID} AND
    mirror=true AND
	use_custom_mirror_algorithm=true
ORDER BY
	source_schema_name, 
    source_table_name</custom_sql>
