  <step>
    <name>Load list of tables to compare</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>etl_db</connection>
    <sql>SELECT
    t.table_meta_id,
    t.source_schema_name AS "source_schema",
    t.source_table_name AS "source_table",
    t.source_schema_name || '.' || t.source_table_name AS "source_schema_dot_table",
    t.target_schema_name AS "target_schema",
    t.target_table_name  AS "target_table",
    t.target_schema_name || '.' || t.target_table_name AS "target_schema_dot_table",
    t.row_can_be_deleted_from_seconds AS "row_can_be_deleted_from_seconds",
    t.use_custom_delete_algorithm,
    t.compare_before_delete,
    (SELECT
        c.source_column_name 
     FROM
         etl.column_meta c
     WHERE
         c.table_meta_id=t.table_meta_id AND c.is_row_can_be_deleted_from_column=true
    ) AS "row_can_be_deleted_from_source_colname",
    (SELECT
        c.source_column_name 
     FROM
        etl.column_meta c
     WHERE
        c.table_meta_id=t.table_meta_id AND c.is_can_delete_row_column=true
    ) AS "can_delete_row_source_colname",
    (SELECT
        c.source_column_name 
     FROM
        etl.column_meta c
     WHERE
        c.table_meta_id=t.table_meta_id AND c.is_last_updated_on_column=true
    ) AS "last_updated_on_source_colname"
FROM
    etl.table_meta t
WHERE
    t.source_db_id=${PARAM_SOURCE_DB_ID} AND
    (
        (t.use_custom_delete_algorithm=false AND
         t.row_can_be_deleted_from_seconds IS NOT NULL)  -- "Generic" delete algorithm will be used
        OR t.use_custom_delete_algorithm=true            -- "Custom"  delete algorithm will be used
    )
ORDER BY
    t.source_schema_name,
    t.source_table_name</sql>
