        <class_source>public static class Fields {
	public static class In  { public static FieldHelper product; }
}

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
        data.outputRowMeta = getInputRowMeta().clone();
        meta.getFields(data.outputRowMeta, getStepname(), null, null, parent);
		Fields.In.product = new FieldHelper(data.inputRowMeta, "product");
    }

	if ("firefox".equals(Fields.In.product.getString(r))) {
		Fields.In.product.setValue(r, "fennec-1.0b5");
        incrementLinesUpdated();
	}

    putRow(data.outputRowMeta, r);
	return true;
}

