      <name>Update etl.run row to show progress</name>
      <description />
      <type>SQL</type>
      <attributes />
      <sql>UPDATE 
    ${QF_ETL_DB_SCHEMA}.etl_run 
SET 
     progress_source_db_id=${PARAM_SOURCE_DB_ID},
     progress_source_schema_name='${PARAM_SOURCE_SCHEMA}',
     progress_source_table_name='${PARAM_SOURCE_TABLE}',
     progress_source_column_name_insert_id=(SELECT source_column_name FROM etl.column_meta cm WHERE cm.table_meta_id=${PARAM_TABLE_META_ID} AND cm.is_insert_id_column=true),
     progress_source_column_name_last_updated_on=(SELECT source_column_name FROM etl.column_meta cm WHERE cm.table_meta_id=${PARAM_TABLE_META_ID} AND cm.is_last_updated_on_column=true),
     progress_target_db_id=${PARAM_TARGET_DB_ID},
     progress_target_schema_name='${PARAM_TARGET_SCHEMA}',
     progress_target_table_name='${PARAM_TARGET_TABLE}',
     progress_etl_operation=NULL,
     progress_insert_id=NULL,
     progress_last_updated_on=NULL,
     progress_rows_processed_per_update=NULL,
     progress_rows_processed =NULL,
     progress_rows_to_process_remaining=NULL,
     progress_rows_processed_per_second=NULL,
     progress_updated_at_utc=now() at time zone 'utc'
WHERE
     etl_run_id=${PARAM_ETL_RUN_ID}
</sql>
