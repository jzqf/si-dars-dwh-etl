    <connection>generic_source_db</connection>
    <sql>WITH cte AS (
    SELECT 
         row_number() OVER (ORDER BY cdc_id) - 1                                                AS "rows_deleted", 
        (row_number() OVER (ORDER BY cdc_id) - 1) / ${PARAM_CDC_CONTROL_TABLE_BATCH_SIZE} + 1  AS "counter", 
        (row_number() OVER (ORDER BY cdc_id) - 1) % ${PARAM_CDC_CONTROL_TABLE_BATCH_SIZE}      AS "reset_each_batch" ,
         CEIL(COUNT(*) OVER () / FLOAT4(${PARAM_CDC_CONTROL_TABLE_BATCH_SIZE}))                AS "counter_max",
              COUNT(*) OVER ()                                 AS "rows_to_delete"
    FROM 
