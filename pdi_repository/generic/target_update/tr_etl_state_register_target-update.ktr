    <connection>generic_target_db</connection>
    <sql>SELECT
    '${PARAM_TARGET_SCHEMA}' AS "schema_name", 
    '${PARAM_TARGET_TABLE}'  AS "table_name", 
    NULLIF('${PARAM_TARGET_INSERT_ID_COLNAME}'                  , ''                                       ) AS "dwh_last_updated_insert_id_colname", 
