  <step>
    <name>Get "table_last_changed_on" for source table</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>generic_source_db</connection>
    <sql>SELECT 
    table_last_changed_on
FROM 
    ${QF_ETL_CDC_TRUNCATE_HELPER_SCHEMA_NAME}.${QF_ETL_CDC_TRUNCATE_HELPER_TABLE_NAME} 
WHERE
    schema_name = '${PARAM_SOURCE_SCHEMA}' AND
    table_name  = '${PARAM_SOURCE_TABLE}'

UNION

-- This query is needed so that we set a value for the "table_last_changed_on" 
-- stream for the case there is no matching "etl_cdc_truncate_helper" row for 
-- the source table. For this case we return a timestamp from long in the past, 
-- which should force the truncate-reload algorithm to run once, but not on 
-- future attempts because this value will be stored in the etl.table_state 
-- table in the row the current source table. Normally, this second SELECT
-- statement will not return a row because the row should be selected in the
-- first query.
SELECT
--  CAST ('${QF_TIMESTAMP_NOT_SET_VALUE}' AS TIMESTAMP) AS "table_last_changed_on"
    CAST ('2000-01-01 00:00:00' AS TIMESTAMP) AS "table_last_changed_on"
WHERE NOT EXISTS (
    SELECT 
        table_last_changed_on
    FROM 
        ${QF_ETL_CDC_TRUNCATE_HELPER_SCHEMA_NAME}.${QF_ETL_CDC_TRUNCATE_HELPER_TABLE_NAME} 
    WHERE
        schema_name = '${PARAM_SOURCE_SCHEMA}' AND
        table_name  = '${PARAM_SOURCE_TABLE}'    
)</sql>
