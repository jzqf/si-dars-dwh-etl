    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>/*
This script treats two different use cases:

Case 1: PARAM_INSERT_ID_FROM &lt;= 0

  This is the originlk functionality implemented in this step where 
  PARAM_INSERT_ID_FROM is *not used* and the most *recent* rows from the
  specified table are compared.

  PARAM_MAX_NUM_COMPARE_ROWS: 

    This parameter specifies the maximum number of the most recent rows to 
    compare. "Recent" means the largest values of the the "insert id" column. 
    If this value is zero, then *all* rows will be compared. If this value is 
    non-zero, then the field "insert_id_from" will be increased, if necessary, 
    so that the specified number of rows will be compared.

Case 2: PARAM_INSERT_ID_FROM > 0

  This case makes use of the values of *both* PARAM_INSERT_ID_FROM and
  PARAM_MAX_NUM_COMPARE_ROWS. For this case of value of the stream fields 
  "insert_id_from" &amp; "insert_id_to" are adjusted so that:

    insert_id_from	= PARAM_INSERT_ID_FROM
    insert_id_to		= insert_id_from + PARAM_MAX_NUM_COMPARE_ROWS

  Author:  Jeffrey Zelt
  Date:    2017.06.29
  Changes: Initial version

  Author:  Jeffrey Zelt
  Date:    2020.03.03
  Changes: Added support for PARAM_INSERT_ID_FROM
*/

//Alert("insert_id_from = " + insert_id_from + "\ninsert_id_to = " + insert_id_to);

var param_insert_id_from = parseInt(getVariable("PARAM_INSERT_ID_FROM", "-1"));
var param_max_num_compare_rows = parseInt(getVariable("PARAM_MAX_NUM_COMPARE_ROWS", "0"));
//Alert("param_insert_id_from = " + param_insert_id_from + "\nparam_max_num_compare_rows = " + param_max_num_compare_rows);

if(param_insert_id_from &lt;= 0) {

  // Case 1:

  if(param_max_num_compare_rows > 0) {
    var numRows = insert_id_to - insert_id_from + 1;
    //Alert("numRows = " + numRows);
    if(numRows > param_max_num_compare_rows){
      insert_id_from = insert_id_to - param_max_num_compare_rows + 1;
    }
  }

} else {

  // Case 2:

  // This test avoids assigning a value to insert_id_from that is less than 
  // the minimum value of the "insert id" column from the source table.
  // Note that insert_id_from may be -1 here if the source table is empty.
  if(param_insert_id_from >= insert_id_from) {
    insert_id_from = param_insert_id_from;
  }

  // This test avoids assigning a value to insert_id_to that is greater than 
  // the maximum value of the "insert id" column from the target table.
  // Note that insert_id_to may be -1 here if the target table is empty.
  if((insert_id_from + param_max_num_compare_rows) - 1 &lt; insert_id_to) {
    insert_id_to = insert_id_from + param_max_num_compare_rows - 1;
  }

}

//Alert("insert_id_from = " + insert_id_from);
//Alert("insert_id_to = " + insert_id_to);
</jsScript_script>
      </jsScript>
    </jsScripts>
